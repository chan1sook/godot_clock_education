;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Clock Education"
config/description="Clock Education Game"
config/version="0.0.4"
run/main_scene="res://scenes/main_menu.tscn"
config/custom_user_dir_name="Clock Education"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://assets/logo.png"
config/windows_native_icon="res://assets/logo.ico"

[autoload]

Global="*res://global.gd"

[display]

window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
